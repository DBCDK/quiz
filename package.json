  "license": "GPL-3.0-or-later",
  "author": "Rasmus Erik Voel Jensen, solsort.com ApS",
  "homepage": "https://dbcdk.github.io/quiz/",
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^2.0.3",
    "immutable": "^4.0.0-rc.9",
    "jss": "^9.8.7",
    "mustache": "^2.3.2",
    "react": "^16.4.1",
    "react-beautiful-dnd": "^9.0.2",
    "react-dom": "^16.4.1",
    "react-jss": "^8.6.1",
    "react-redux": "^5.0.7",
    "react-scripts": "1.1.4",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "gh-pages": "^1.2.0",
    "lint-staged": "^7.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.7",
    "react-test-renderer": "^16.4.1"
  },
  "pre-commit": [
    "prettier"
  ],
  "lint-staged": {
    "*.{js,md,json,css}": [
      "prettier --no-bracket-spacing --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "prettier": "lint-staged",
    "start": "react-scripts start",
    "build": "rm build/static/js/main.*.js; react-scripts build; cd build; ln -sf ./static/js/main.*.js widget.js; ln -sf ./static/js/main.*.js admin.js",
    "test": "react-scripts test --env=jsdom",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  }
}
